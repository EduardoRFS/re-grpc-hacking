{
  "name": "grpc",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "dune build -p <PACKAGE_NAME>",
    "buildDev": "pesy",
    "release": { "releasedBinaries": [ "ReJwtApp.exe" ] }
  },
  "buildDirs": {
    "examples/client": {
      "require": [ "grpc.lib", "grpc.protos" ],
      "preprocess": [ "pps", "ppx_let" ],
      "main": "Client",
      "name": "Client.exe"
    },
    "examples/server": {
      "require": [ "grpc.lib", "grpc.protos" ],
      "preprocess": [ "pps", "ppx_let" ],
      "main": "Server",
      "name": "Server.exe"
    },
    "lib": {
      "require": [
        "console.lib", "base", "stdio", "h2", "h2-lwt-unix", "lwt_ssl",
        "lwt", "ocaml-protoc"
      ],
      "preprocess": [ "pps", "ppx_let" ],
      "namespace": "GRPC",
      "name": "grpc.lib"
    },
    "protos": {
      "require": [ "ocaml-protoc" ],
      "preprocess": [ "pps", "ppx_let" ],
      "namespace": "Protos",
      "name": "grpc.protos"
    }
  },
  "scripts": { "pesy": "bash -c 'env PESY_MODE=update pesy'" },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/base": "v0.12.2",
    "@opam/dune": ">=1.6.0",
    "@opam/h2": "2.0.0",
    "@opam/h2-lwt": "2.0.0",
    "@opam/h2-lwt-unix": "2.0.0",
    "@opam/lwt": "4.3.1",
    "@opam/lwt_ssl": "1.1.3",
    "@opam/ocaml-protoc": "1.2.0",
    "@opam/ppx_let": "v0.12.0",
    "@reason-native/console": "^0.1.0",
    "grpcc": "^1.1.3",
    "ocaml": "^4.4.0"
  },
  "devDependencies": {
    "ocaml": "4.7.1004",
    "refmterr": "*",
    "pesy": "*",
    "@opam/merlin": "*"
  },
  "resolutions": {
    "@opam/h2":
      "EduardoRFS/ocaml-h2:h2.opam#7e4e51a001380e0152d1a9cb6e99b087a14b777d",
    "@opam/h2-lwt": "link:/home/eduardo/workspace/ocaml-h2/h2-lwt.opam",
    "@opam/h2-lwt-unix":
      "link:/home/eduardo/workspace/ocaml-h2/h2-lwt-unix.opam"
  }
}